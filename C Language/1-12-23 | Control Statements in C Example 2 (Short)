#include <stdio.h>

int main(void){
    int age;


    printf("What is your age?\n");
    scanf("%d", &age);

    int isInTheWorkForce = (age >= 18) && !(age >= 65);

    if(isInTheWorkForce){
        printf("You are in the labor force.\n");
    }
    else
    {
        printf("You are not in the labor force.\n");
    }

    return 0;
}
