#include <stdio.h>

void printArray(int *, int);

int main(){
    int array[] = {10, 20, 30, 40, 50};
    int N = 5;
    printArray(array, N);
    return 0;
}

void printArray(int * ptr, int size){
    int i;
    for(i=0; i<size; i++){
        printf("%d ", *(ptr + i));
    }
    printf("\n"); //line spacing
}
