#include <stdio.h>
int main() {
int score = 42;
int * scorePointer = &score;
double percentage = 78.45;
double * percentagePointer = &percentage;
char letter = 'B';
char * letterPointer = &letter;
printf("The score is %d and its address is %p.\n", *scorePointer,
scorePointer);
printf("The percentage is %lf and its address is %p.\n",
*percentagePointer, percentagePointer);
printf("The letter is %c and its address is %p.\n", *letterPointer,
letterPointer);
return 0;
}
