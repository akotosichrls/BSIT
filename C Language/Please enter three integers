//Please enter three integers



#include <stdio.h>

int main(){
    int first;
    int second;
    int third;
    printf("Please enter three integers.\n");
    scanf("%d%d%d",&first,&second,&third);
    printf("You entered: %d for first, %d for second, and %d for third.\n",first,second,third);
    return 0;
}
